{
  "mappings": {
    "_default_": {
      "_all": {
        "enabled": false,
          "norms": {
            "enabled": false
          }
      },
      "properties": {
        "target": {
          "properties": {
            "InstanceId": {
              "type": "string",
              "index": "not_analyzed"
            },
            "VolumeId": {
              "type": "string",
              "index": "not_analyzed"
            },
            "tags":{
              "properties":{
                "Component": {
                  "type": "string",
                  "index": "not_analyzed"
                },
                "Name": {
                  "type": "string",
                  "index": "not_analyzed"
                }
              }
            }
          }
        }
      }
    }
  },
  "settings": {
    "index.refresh_interval": "5s",
    "number_of_shards": 2
  },
  "template": "ec2-*",
  "order": 1
}
